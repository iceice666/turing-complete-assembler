# ! alias N r0
# ! alias D r1
# ! alias Q r2
# ! alias R r3
# ! alias cmp r4
# ! alias zero r5
# ! alias one r6

# ! label init
0x48 0x00 0x00 0x00 # li N [ln.1]
0x48 0x01 0x00 0x00 # li D [ln.2]
0x04 0x03 0x00 0x00 # mov R N [ln.3]
0x04 0xA2 0x00 0x00 # mov Q zero [ln.4]
0x04 0xA6 0x00 0x01 # addi one zero 1 [ln.5]


# ! label mainloop
# cmp <- R < D
0x00 0x61 0x20 0x09 # slt cmp R D [ln.6]

0x80 0xC4 0x00 0x03 # beq cmp one exit [ln.7]

# div
0x00 0x61 0x18 0x02 # sub R R D [ln.8]
0x04 0x42 0x00 0x01 # addi Q Q 1 [ln.9]
0x80 0x00 0xFF 0xFB # goto mainloop [ln.10]


# ! label exit
0x4C 0x02 0x00 0x00 # so Q [ln.11]
0x4C 0x03 0x00 0x00 # so R [ln.12]